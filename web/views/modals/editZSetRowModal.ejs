<div id="editZSetRowModal" class="modal hide fade">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">Ã—</button>
    <h3>Edit Row</h3>
  </div>
  <div class="modal-body">
    <form id="editZSetRowForm" action="apiv1/editZSetRow/" method="POST" class="form-vertical label-big">
      <label class="control-label lead" for="zSetKey">Key</label>
      <input id="zSetKey" name="zSetKey" type="text" readonly class="form-control" /
      <label class="control-label lead" for="zSetScore">Score</label>
      <input id="zSetScore" name="zSetScore" type="text" class="form-control" />
      <label class="control-label lead" for="zSetValue">Value</label>
      <textarea id="zSetValue" name="zSetValue" style="width: 500px; height: 150px;"></textarea>
      <input id="zSetOldValue" name="zSetOldValue" hidden class="form-control" />
      <input id="zSetConnectionId" name="zSetConnectionId" hidden class="form-control" />
      <br>
    </form>
  </div>
  <div class="modal-footer">
    <button class="btn btn-default" data-dismiss="modal">Close</button>
    <button id="removeZSetValueButton" class="btn btn-danger" data-loading-text="<i class='icon-refresh'></i> Removing..." onclick="removeZSetElement()">Remove Element</button>
    <button id="editZSetValueButton" class="btn btn-primary" data-loading-text="<i class='icon-refresh'></i> Saving..." onclick="$('#editZSetRowForm').submit()">Save</button>
  </div>
</div>
